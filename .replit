modules = ["python-3.11"]

[nix]
channel = "stable-24_05"
packages = ["libxcrypt"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "MemoryOS Production Server"

[[workflows.workflow]]
name = "MemoryOS Production Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python deploy_server.py"
waitForPort = 5000

[deployment]
deploymentTarget = "cloudrun"
run = ["sh", "-c", "python deploy_server.py"]

[[ports]]
localPort = 3000
externalPort = 3000

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 5001
externalPort = 3001

[[ports]]
localPort = 5002
externalPort = 3002
